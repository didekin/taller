<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>taller</artifactId>
    <groupId>com.lebendata.taller</groupId>
    <version>2.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.lebendata.taller.bosch</groupId>
  <artifactId>bosch_aws</artifactId>
  <version>2.0-SNAPSHOT</version>
  <url>https://github.com/didekin/lebendata_taller</url>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>${plugin.shade}</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <transformers>
            <transformer>
              <mainClass>com.lebenlab.core.TallerBoschApp</mainClass>
              <manifestEntries>
                <Multi-Release>true</Multi-Release>
              </manifestEntries>
            </transformer>
          </transformers>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>org.jdbi</groupId>
      <artifactId>jdbi3-bom</artifactId>
      <version>3.17.0</version>
      <type>pom</type>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.13.1</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>hamcrest-core</artifactId>
          <groupId>org.hamcrest</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.assertj</groupId>
      <artifactId>assertj-core</artifactId>
      <version>3.18.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>com.konghq</groupId>
      <artifactId>unirest-java</artifactId>
      <version>3.11.05</version>
      <classifier>standalone</classifier>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>httpmime</artifactId>
          <groupId>org.apache.httpcomponents</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.awaitility</groupId>
      <artifactId>awaitility</artifactId>
      <version>4.1.0</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>hamcrest</artifactId>
          <groupId>org.hamcrest</groupId>
        </exclusion>
      </exclusions>
    </dependency>
  </dependencies>
  <properties>
    <smile.version>2.5.3</smile.version>
    <tablesaw.core.version>0.38.1</tablesaw.core.version>
    <commons_csv.version>1.8</commons_csv.version>
    <hikariCP.version>3.4.5</hikariCP.version>
    <jose4j.version>0.7.7</jose4j.version>
    <gson.version>2.8.6</gson.version>
    <assertj.version>3.18.1</assertj.version>
    <awaitility.version>4.1.0</awaitility.version>
    <log4j.version>2.14.0</log4j.version>
    <junit.version>4.13.1</junit.version>
    <jdbi.core.version>3.17.0</jdbi.core.version>
    <sinch.version>1.0.4</sinch.version>
    <javalin.version>3.12.0</javalin.version>
    <mysql.connector.version>8.0.22</mysql.connector.version>
    <unirest.version>3.11.05</unirest.version>
    <velocity.version>2.2</velocity.version>
  </properties>
</project>
